{
  "extends": ["plugin:node/recommended", "plugin:jest/recommended", "eslint-config-airbnb-typescript", "prettier", "eslint-config-prettier"],
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}